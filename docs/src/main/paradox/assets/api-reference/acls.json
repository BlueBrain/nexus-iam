{
  "acl": [
    {
      "permissions": [
        "read"
      ],
      "identity": {
        "@id": "https://example.com/realms/realm",
        "realm": "realm",
        "@type": "AuthenticatedRef"
      }
    },
    {
      "permissions": [
        "read", "write"
      ],
      "identity": {
        "@id": "https://example.com/realms/realm/groups/some-group",
        "realm": "realm",
        "group": "some-group",
        "@type": "GroupRef"
      }
    },
    {
      "permissions": [
        "read", "write", "own"
      ],
      "identity": {
        "@id": "https://example.com/realms/realm/users/user",
        "realm": "realm",
        "sub": "alice",
        "@type": "UserRef"
      }
    }
  ]
}